{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "Node",
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "types": [
        "vite/client", 
        "@types/gtag.js"
      ],
      "paths": {
        "@/*": ["./.vitepress/*"]
      }
    },
    "include": [
      ".vitepress/theme/**/*",
      "node_modules/vitepress/dist/client/theme-default/components/**/*.vue"
    ]
}